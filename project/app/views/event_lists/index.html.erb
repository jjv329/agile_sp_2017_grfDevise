<h1>Events</h1>

<%= render "/shared/search" %>

<table id="eventsTable">
  <thead  id="eventTitles">
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Name</th>
      <th>Description</th>
      <th>Location</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Number Of Riders</th>
      <th>Vehicle(s)</th>
      <th>Pick Up Address</th>
      <th>Pick Up City</th>
      <th>Pick Up State</th>
      <th>Pick Up Zip</th>
      <th>Smoking Allowed</th>
      <th>Created By</th>
      <th>Driver</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_lists.each do |event_list| %>
      <tr>
        <td><%= event_list.eventDate %></td>
        <td><%= event_list.eventTime.to_s(:time) %></td>
        <td><%= event_list.eventName %></td>
        <td><%= event_list.eventDescription %></td>
        <td><%= event_list.streetAddress %></td>
        <td><%= event_list.City %></td>
        <td><%= event_list.State %></td>
        <td><%= event_list.Zip %></td>
        <td><%= event_list.nbrOfRiders %></td>
        <td><%= event_list.vehicleType %></td>
        <td><%= event_list.PUstreetAddress %></td>
        <td><%= event_list.PUCity %></td>
        <td><%= event_list.PUState %></td>
        <td><%= event_list.PUZip %></td>
        <td><%= event_list.smokingAllowed %></td>
        <td><%= event_list.createdBy %></td>
        <td><%= event_list.eventDriver %></td>
        <td><%= link_to 'Show', event_list %></td>
        <td><%= link_to 'Edit', edit_event_list_path(event_list) %></td>
        <td><%= link_to 'Destroy', event_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
